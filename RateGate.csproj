<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <AssemblyName>CVV.RateGate</AssemblyName>
    <RootNamespace>CVV</RootNamespace>
    <PackageProjectUrl>https://github.com/clayv/RateGate</PackageProjectUrl>
    <RepositoryUrl>https://github.com/clayv/RateGate</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <Description>This is Jack Leitch's RateGate code with a couple improvements specifically around thread saftety and long interval times (e.g. &gt; 24.9 days).</Description>
    <PackageReleaseNotes>Initial release</PackageReleaseNotes>
    <Version>1.0.1</Version>
    <EnforceCodeStyleInBuild>True</EnforceCodeStyleInBuild>
    <EnableNETAnalyzers>True</EnableNETAnalyzers>
    <AnalysisLevel>preview-all</AnalysisLevel>
  </PropertyGroup>

  <ItemGroup>
    <Reference Include="CVV.Disposable">
      <HintPath>..\Disposable\bin\Release\netstandard2.1\CVV.Disposable.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
